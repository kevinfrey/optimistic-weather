{
  "expo": {
    "name": "Optimistic Weather",
    "slug": "optimistic-weather-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-location"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.optimistic-weather-mobile",
      "icon": "./assets/icon.png"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "extra": {
      "EXPO_PUBLIC_OPENWEATHER_API_KEY": "03ff0bcf4c277b582b64cb1d0ed11391"
    }
  }
}
